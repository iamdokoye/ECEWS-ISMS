{
    "version": 2,
    "builds": [
        {
            "src": "frontend/staff-view/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/frontend/staff-view/index.html"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "frontend/staff-view/$1"
        }
    ]
}